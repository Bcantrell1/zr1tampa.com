package services

templ ServiceCollapse(title string, content string) {
	<div class="mx-auto mb-2 max-w-screen-xl px-4 py-2 sm:px-6 sm:py-2 lg:px-8">
		<div class="collapse collapse-arrow bg-base-200">
			<input type="checkbox" /> 
			<div class="collapse-title text-xl font-medium">
				{title}
			</div>
			<div class="collapse-content"> 
				<p>{content}</p>
			</div>
		</div>
	</div>
}
